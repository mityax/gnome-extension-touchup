<?xml version="1.0" encoding="UTF-8"?>

<!--
All arbitrary assets for this extension need to be defined in this file in order to be bundled
with the extension. They can be loaded anywhere in the extension using the utilities provided
by Gio, e.g.:

```
import {assetPath} from "$src/config.ts";

// Load an arbitrary file:
const myFile = Gio.File.new_for_uri(assetPath.root + '/my-file.txt'));

// Load an icon using the `AssetIcon` helper:
const myIcon = St.Icon({
    gicon: new AssetIcon('camera-flash-symbolic'),
});
```
-->

<gresources>
    <!-- Icons -->
    <gresource prefix="/org/gnome/shell/extensions/gnometouch/icons/scalable/actions/">
        <file preprocess="xml-stripblanks" alias="camera-flash-symbolic.svg">icons/camera-flash-symbolic.svg</file>
    </gresource>
</gresources>
